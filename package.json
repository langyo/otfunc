{
  "name": "otfunc",
  "version": "0.1.0",
  "description": "A JavaScript library to describe the overrided function and typical arguments.",
  "main": "main.bundle.js",
  "keywords": [
    "javascript",
    "functional"
  ],
  "scripts": {
    "watch": "watchify main.js -t babelify -o main.bundle.js --release --verbose",
    "build": "browserify main.js -o main.bundle.js -t babelify -d",
    "debug:watch": "watchify test.js -t babelify -o test.bundle.js --release --verbose",
    "debug:build": "browserify test.js -o test.bundle.js -t babelify -d",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langyo/otfunc.git"
  },
  "author": "langyo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/langyo/otfunc/issues"
  },
  "homepage": "https://github.com/langyo/otfunc",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current",
            "chrome": "53",
            "ie": "11"
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "watchify": "^3.11.1",
    "jest": "^24.1.0"
  }
}
